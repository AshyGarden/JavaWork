
public class Identifier {
	//식별하다 = 분별하여 알아보다
	
	public static void main(String[] args) {
		//꼭 지켜야 할 규칙(안 지킬시 에러가 발생할수 있습니다.)
		//1. 식별자의 이름은 중복을 허용하지 않습니다.
		//그리고 대소문자를 철저하게 구분합니다.
		
		int age = 35;
//		int age = 40;(x)
		int Age = 40;
		
		System.out.println(age);
		System.out.println(Age);
		
		
		
	}

}
